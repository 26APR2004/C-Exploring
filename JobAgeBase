/* 
Take the age from the user and then decide accordingly
1. if age<18,
   print -> "not eligible for job."
2. if age>=18;
   print ->"eligible for job."
3. if age>=55 & age<=57;
   print ->"eligible for job,but retirment is soon."
4. if age>57;
   print ->"retirment time."
*/

# include<bits/stdc++.h>
using namespace std;
int main()
{
    int age;
    cin>>age;
    if(age<18)
    {
     cout<<"not eligible for job!";
    }
    else if(age>=18 && age<55)
    {
     cout<<"eligible for job";
    }
    else if(age>=55 && age<=57)
    {
     cout<<"eligible for job!, but your retirment is soon#";
    }
    else if(age>57)
    {
     cout<<" retirment time";
    }
    return 0;
}
